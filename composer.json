{
    "name": "boostudio/booship",
    "description": "wrapper",
    "keywords": [
        "boostudio",
        "booship"
    ],
    "homepage": "https://github.com/boostudio/booship",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Scott",
            "email": "scott@boostudio.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^8",
        "laminas/laminas-soap": "^2.9"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "BooStudio\\BooShip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BooStudio\\BooShip\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BooStudio\\BooShip\\BooShipServiceProvider"
            ],
            "aliases": {
                "BooShip": "BooStudio\\BooShip\\BooShipFacade"
            }
        }
    }
}