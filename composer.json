{
    "name": "boostudio/booship",
    "description": "Wrapper to access BooShipping platform",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott",
            "email": "scott@boostudio.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "symfony/var-dumper": "2.*"
    },
    "autoload": {
        "psr-4": {
            "BooStudio\\BooShip\\": "src",
            "BooStudio\\Library\\": "src/library"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nextpack\\Nextpack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}